{
    "manifest_version": 3,
    "name": "Chess.com Ad Blocker",
    "version": "0.1",
    "description": "A simple extension to remove ads.",
    "permissions": [
      "scripting"
    ],
    "host_permissions": [
      "*://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }